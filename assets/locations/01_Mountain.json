{
  "Changes": [
    {
      "Logname": "Mountain_base_day",
      "Action": "EditMap",
      "Target": "Maps/Mountain",
      "FromFile": "assets/locations/tmx/01_Mountain_base.tmx"
    },
    {
      "Logname": "Mountain_base_allow_passage",
      "Action": "EditMap",
      "Target": "Maps/Mountain",
      "FromFile": "assets/locations/tmx/01_Mountain_base_allow_passage.tmx",
      "ToArea": {
        "X": 0,
        "Y": 0,
        "Width": 7,
        "Height": 6
      },
      "When": {
        "HasFlag": "nmRemoveLastBoulder"
      }
    },
    {
      "Logname": "Mountain_base_remove_barrier",
      "Action": "EditMap",
      "Target": "Maps/Mountain",
      "FromFile": "assets/locations/tmx/01_Mountain_base_remove_barrier.tmx",
      "ToArea": {
        "X": 45,
        "Y": 6,
        "Width": 1,
        "Height": 4
      },
      "When": {
        "HasFlag": "nmRemoveBarrier"
      }
    },
    {
      "Logname": "Mountain_base_night",
      "SetProperties": {
        "CurrentTime": "{{Time}}"
      },
      "Action": "EditMap",
      "Target": "Maps/Mountain",
      "FromFile": "assets/locations/tmx/01_Mountain_base_night.tmx",
      "When": {
        "Time": "{{Range: 2000, 2600}}"
      },
      "Update": "OnTimeChange"
    },
    {
      "Logname": "Mountain_Map_Properties",
      "Action": "EditMap",
      "Target": "Maps/Mountain",
      "MapProperties": {
        "AllowGrassSurviveInWinter": false,
        "Outdoors": true,
        "forceLoadPathLayerLights": true,
        "Warp": "{{MountainWarps}} {{MountainWarpsSVE}}"
      }
    }
  ]
}